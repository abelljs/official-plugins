<html>
  <head>
    <style>
      body {
        font-family: arial;
      }
    </style>
  </head>
  <body>
    <h1>DEV.to Articles</h1>
    {{
      Abell.contentArray
        .map(meta => /* html */ `
        <div>
          ${meta.title}
          ${meta.$createdAt.toDateString()}
          <a href="${meta.$path}">${meta.$path}</a>
        </div>
        `)
    }}

    <section>
      <h2>Image without alt tag</h2>
      <img width="400" src="https://res.cloudinary.com/saurabhdaware/image/upload/v1588342001/abell/og.png"/>
    </section>

    <section>
      <h2>Image with alt tag</h2>
      <img width="400" alt="Abell Minima Cover" src="https://res.cloudinary.com/saurabhdaware/image/upload/v1588342001/abell/og.png"/>
    </section>
  </body>
</html>